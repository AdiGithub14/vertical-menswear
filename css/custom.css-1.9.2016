/* CSS Document */



/*===============================================================
common portion start
=================================================================
*/



@font-face {
	font-family: DinC;
	src: url(../fonts/DinC.ttf)
}
* {
	box-sizing: border-box
}
body {
	position:relative;
	margin: 0;
	padding: 0 !important;
	font-family: 'PT Sans', sans-serif;
	color: #6a6a6a;
}
body, html {
	font-size: 16px;
}
div, table, tr, th, td, th, video, audio, h1, h2, h3, h4, h5, h6, aside, canvas, section, header, footer, nav, span, p, small, big, a, b, strong, img, hr, pre, button, input, textarea, optgroup, ul, li, ol, select, option, blockquote, nav {
	margin: 0;
	padding: 0;
	outline: none;
	border: none;
	text-decoration: none;
	list-style-type: none;
}
.clearfix:after {
	content: "";
	display: block;
	height: 0;
	clear: both;
	font-size: 0;
}
.clear {
	clear: both
}
.container {
	margin: 0 auto;
	padding: 0 0px;
}
.row {
	width: 100%;
	margin: 10px auto
}
img {
	max-width: 100%;
}
h1, h2, h3, h4, h5, h6 {
	font-family: DinC;
	font-weight: normal;
	word-spacing: 10px;
	color: #2a2a2a;
}
h1 {
	font-size: 3.125rem;
	font-weight: normal;
	text-transform: uppercase;
	font-weight: normal
}
h2 {
	font-size: 2.75rem;
	line-height: 58px;
	font-weight: normal;
	text-transform: uppercase
}
h3 {
	font-size: 2.125rem;
	line-height: 40px;
	text-align: center;
	font-weight: normal;
	text-transform: uppercase
}
h4 {
	font-size: 1.688rem;
	line-height: 36px;
	font-weight: normal;
}
h5 {
}
h6 {
}
p {
	font-size: 16px;
	line-height: 25px;
	font-weight: normal;
	color: #6a6a6a;
	margin: 15px 0
}
input, textarea, select {
	width: 100%;
	height: 40px;
	border: none;
	margin: 0.125rem 0;
	border-radius: 0;
	text-indent: 5px;
	transition: all .5s ease;
	-moz-transition: all .5s ease;
	-webkit-transition: all .5s ease;
	-o-transition: all .5s ease;
	-ms-transition: all .5s ease;
	color: #575555;
	font-size: 15px;/*background:rgba(255,255,255,0.33);*/
}
input:focus, textarea:focus, select:focus {
	background: rgba(255,255,255,0.93);
}
input[type="submit"] {
	width: 100%;
	background: transparent;
	color: #ffffff;
	text-align: center;
	line-height: 45px;
	font-size: 18px;
	cursor: pointer;
	transition: all .5s ease;
	-moz-transition: all .5s ease;
	-webkit-transition: all .5s ease;
	-o-transition: all .5s ease;
	-ms-transition: all .5s ease;
}
input[type="submit"]:hover {
	background: #FFF;
	color: #f8db3a;
}

a:focus {
    outline:none !important;
}
a:hover {
    text-decoration: none !important;
}		
	
	
	
::-moz-placeholder {
 color:#f3f3f3;
}
::-ms-placeholder {
 color:#f3f3f3
}
::-webkit-placeholder {
 color:#f3f3f3
}
::-0-placeholder {
 color:#f3f3f3
}
::selection {
	background: #f8db3a;
	color: #050006;
}
::-moz-selection {
 background:#f8db3a;
 color:#050006;
}
::-webkit-selection {
 background:#f8db3a;
 color:#050006;
}
::-o-selection {
 background:#f8db3a;
 color:#050006;
}
::-ms-selection {
 background:#f8db3a;
 color:#050006;
}
.f_left {
	float: left;
	text-align: left
}
.f_right {
	float: right;
	text-align: right
}
a {
    color: #000;
    text-decoration: none;
}
a.btn_bor {
	background: transparent;
	color: #ffffff;
	text-align: center;
	line-height: 45px;
	font-size: 18px;
	transition: all .3s ease;
	-moz-transition: all .3s ease;
	-webkit-transition: all .3s ease;
	-o-transition: all .3s ease;
	-ms-transition: all .3s ease;
	margin-left: auto;
	margin-right: auto;
	display: block;
	border-radius: 25px;
	border: 1px solid #acacac;
}
a.btn_bor:hover {
	background: rgba(226,226,226,.3);
}
a.btn_without_bor {
	background: #faa013;
	border-radius: 25px;
	font-family: 'Fjalla One', sans-serif;
	font-size: 15px;
	text-align: center;
}
.read_more {
	color: #fff;
	font-size: 13px;
	font-style: italic
}
.read_more:hover {
	text-decoration: underline
}
a {
	transition: all .3s ease;
	-moz-transition: all .3s ease;
	-webkit-transition: all .3s ease;
	-o-transition: all .3s ease;
	-ms-transition: all .3s ease;
}
a.trans_btn {
	padding: 15px 26px;
	border: 1px solid #2a2a2a;
	color: #2a2a2a;
	background: tranparent;
}
a.black_btn {
	padding: 15px 26px;
	color: #fff;
	background: #2a2a2a;
	border: 1px solid transparent;
}
a.trans_btn:hover {
	background: #2a2a2a;
	color: #fff;
}
a.black_btn:hover {
	border: 1px solid #2a2a2a;
	color: #2a2a2a;
	background: transparent;
}
h2:before {
	content: '';
	width: 100%;
	height: 3px;
	background: #2a2a2a;
	position: absolute;
	top: -10px;
	right: 0
}
h2 {
	position: relative;
	word-spacing: normal !important
}



/*===============================================================
common portion end
=================================================================
*/



/* *************************************************************
************************** Header ******************************
*************************************************************** */

.header_top {
	background: #2a2a2a;
	padding: 10px 0
}
.header_top .content {
	float: right;
	padding: 0
}
.header_top .content li {
	float: left;
	padding: 0 5px;
	color: #fff
}
.header_top .content li a {
	color: #fff;
	font-size: 14px
}
.header-bottom {
	background: #000000;
	padding: 35px 0;
	border-bottom: 4px solid #151515
}
.header-bottom .logo {
	float: left;
}
.header-bottom .top_menu {
	float: right;
}
.header-bottom .top_menu ul {
	padding: 15px 0
}
.header-bottom .top_menu ul li {
	margin: 0 8px;
	color: #fff;
	float: left
}
.header-bottom .top_menu .search {
	width: 22px;
	height: 22px;
	border: 1px solid #fff;
	cursor: pointer
}
.header-bottom .top_menu .search i {
	color: #fff;
	line-height: 22px;
	text-align: center;
	font-size: 11px;
	display: block
}
.header-bottom .top_menu ul li a {
	color: #fff;
	font-size: 13px;
	text-transform: uppercase;
}
.header-bottom .top_menu ul li a span.circle {
	width: 22px;
	height: 22px;
	border: 1px solid #fff;
	border-radius: 50%;
	display: inline-block;
	text-align: center;
	line-height: 20px
}
nav {
	background: #2a2a2a
}
nav ul li {
	float: left;
}
nav ul li a {
	padding: 0 20px;
	border-left: 2px solid #151515;
	color: #fff;
	line-height: 64px;
	font-family: DinC;
	font-size: 14px;
	color: #a4a4a4;
	display: block
}
nav ul li a span {
	color: #fff
}
nav ul li:first-child a {
	border-left: none;
}
nav ul li a:hover {
	color: #fff
}
