}
#flexiselDemo2 li {
	position: relative
}
#flexiselDemo2 li .overlay {
	position: absolute;
	background: rgba(0,0,0,.78);
	width: 100%;
	height: 60px;
	bottom: 0;
	padding: 0 10px
}
#flexiselDemo2 li .overlay .title {
	float: left;
}
#flexiselDemo2 li .overlay .links {
	float: right;
}
#flexiselDemo2 li .overlay .title h6 {
