	font-size: 14px;
	font-family: "PT Sans";
}
.navigation-part4 {
	width: 20%;
	float: left;
}
.menu-bar{
	display:none;
}
.menu-button{
	display:none;
	margin:0;
}
.menu-bar li{
	float:none;
}
.menu-bar li a{
	border-left:hidden;
	line-height: 35px;
}

/*------------- search field ---------------*/

.search_field {
	background: rgba(0,0,0,.9);
	width: 100%;
	height: 100%;
	position: fixed;
	z-index: 9999999;
	display: none;
}
.search_field a {
	display: block;
	width: 40px;
	height: 40px;
	border: 1px solid #fff;
	position: absolute;
	right: 50px;
	top: 50px;
	text-align: center;
}
.search_field a i {
	display: block;
	color: #fff;
	line-height: 40px;
	font-size: 18px
}
.search_box {
	width: 60%;
	margin: 0 auto;
	height: 100%;
	position: relative
}
.search_box .search_box_inside {
	position: absolute;
	width:100%
}


.search_box .search_box_inside .search-area {
	border-bottom:1px solid #515151;
	background: transparent none repeat scroll 0% 0%;
}
.search_box .search_box_inside .submit-button{
	background: transparent none repeat scroll 0% 0%;
	text-align:center;
	margin-top:15px;
	color:#fff;
	border: 1px solid #515151;
	width:100%;
	cursor:pointer;
	line-height:40px;
	transition: all .5s ease;
	-moz-transition: all .5s ease;
	-webkit-transition: all .5s ease;
	-o-transition: all .5s ease;
	-ms-transition: all .5s ease;
}


.search_box .search_box_inside .submit-button:hover{
	background:#000;
	color:#fff
	}

.search_box .search_box_inside .search-area:focus{
	color:#fff
	}

