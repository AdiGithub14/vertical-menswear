}
.navigation-part2 {
	width: 30%;
	float: left;
}
.navigation-part2 h3, .navigation-part3 h3, .navigation-part4 h3 {
	color: #2a2a2a !important;
	font-size: 21px;
	font-family: DinC;
	text-align: left;
	word-spacing: 4px;
