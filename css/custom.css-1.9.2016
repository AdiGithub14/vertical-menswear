	font-size: 16px;
	line-height: 60px
}
#flexiselDemo2 li .overlay .links li {
	float: left;
	padding: 0 5px
}
#flexiselDemo2 li .overlay .links li a {
	color: #fff;
	line-height: 60px
}
/* *************************************************************
************************** season sale ******************************
*************************************************************** */

#season_sale {
	padding: 80px 0;
	background: url(../images/season_pic.png) no-repeat 100px 70px

}
#season_sale .content {
	width: 50%
}
#season_sale .content p {
	margin-bottom: 50px
}
#season_sale h2 {
	margin-top: 28px
}
#season_sale h2:before {
	width: 92%
}
/* *************************************************************
************************** blog section **************************
*************************************************************** */

#blog_section .blog {
	background: url(../images/blog_bg.png) no-repeat 0 0;
	padding: 10% 0 35% 0;
	float: left;
	width: 56%
}
#blog_section .title {
	font-size: 1.688rem;
	color: #2a2a2a;
	position: relative;
	left: 70px;
	top: 0 !important
}
#blog_section .title:before {
	content: '';
	width: 100%;
	height: 3px;
	background: #2a2a2a;
	position: absolute;
	top: -10px;
	left: 0
}
#blog_section .why_about {
	width: 44%;
	float: right
}
#blog_section .why_about .why {
	background: url(../images/why_pic.png) no-repeat right 0;
	padding: 38% 0 10% 0
}
#blog_section .why_about .why a {
	color: #fff !important;
}
#blog_section .why_about .why .title:before {
	background: #fff
}
#blog_section .why_about .about {
	background: #151515;
	padding: 10.1% 40px;
	color: #fff
}
#blog_section .why_about .about h4 {
	color: #fff;
	position: relative
}
#blog_section .why_about .about h4:before {
	content: '';
	width: 26%;
	height: 3px;
	background: #fff;
	position: absolute;
	top: -10px;
	right: 0
}
#blog_section .why_about .about p {
	color: #fff
}
#blog_section .why_about .about .read_more i {
	margin-left: 5px;
	display: inline-block
}
/* *************************************************************
************************** testimonial **************************
*************************************************************** */

#testimonial {
	padding: 50px 0 105px;
}
#testimonial .circle {
	width: 96px;
	height: 96px;
	background: #2a2a2a;
	border-radius: 50%;
	margin: 0 auto;
	text-align: center;
	display: table;
	line-height: 96px;
}
#testimonial .circle img {
	line-height: 96px;
	display: block;
	text-align: center;
	margin: 0 auto;
	vertical-align: middle;
	margin-top: 30%
}
#testimonial h3 {
	margin-top: 20px
}
#testimonial p big {
	font-weight: bold;
	font-size: 16px;
	color: #2a2a2a;
	margin-top: 10px
}
#testimonial p {
	margin: 0 !important
}
#testimonial p {
	margin-top: 15px
}
