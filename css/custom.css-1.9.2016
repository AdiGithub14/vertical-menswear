/* customize shirt */

#header.custom_shirt_header .header-bottom{
	padding:12px 0 !important
	}


#viewport{
	width:100%;
	padding:20px 0;
	overflow:hidden;
  
	}

#viewport .steps_bar{
	background:#292929;
	padding:0 5px;
	overflow:hidden;
	width:115px;	
	height:auto;
	position:relative
	}
#viewport .stepsbar_wrapper{
	display:table;
	height:100%;
	float:left;
	position:absolute;
	left:20px;
	}

#viewport .stepsbar_wrapper .stepsbar_inner_wrapper{
	display:table-cell;
	vertical-align:middle;
	position:relative
	}

a#btn_up{
	display: none;
}

#btn_up, #btn_bottom{
	display:none;
	position:absolute;	
	}
	

#viewport .steps_bar .steps_scroll li{
	height:100px;
	background:#fff;
	width:100%;
	margin:5px 0;
	cursor:pointer;
	text-align:center;
	
	}
	
#viewport .steps_bar .steps_scroll li.active{
	background:#00a2e8;
	
	}	
#viewport .steps_bar .steps_scroll li.active p{
	color:#fff
	}	

#viewport .stepsbar_wrapper .stepsbar_inner_wrapper #btn_up, #viewport .stepsbar_wrapper .stepsbar_inner_wrapper #btn_bottom{
	display:block;
	width:100%;
	text-align:center
	}

#viewport .stepsbar_wrapper .stepsbar_inner_wrapper #btn_up
{
	top:-20px;
	
	}
	#viewport .stepsbar_wrapper .stepsbar_inner_wrapper #btn_up img{
		width:40px;
	}
	#viewport .stepsbar_wrapper .stepsbar_inner_wrapper #btn_bottom img{
		width:40px;
	}

#popups{
	float:left;
	
	}
#popups .popup{
	width:434px;
	background:#000;
	padding:3px;
	height:300px;
	position:absolute;
    left: -100%;
	z-index:999
	}
#popups .close{
	
	position:absolute;
	right:-15px;
	top:-15px;
	color:#f00;
	width:37px;
	height:37px;
	border-radius:50%;
	background:#ccc;
	padding:3px;
	text-align:center;
	z-index:99999;
	transition: all .5s ease;
	-moz-transition: all .5s ease;
	-webkit-transition: all .5s ease;
	-o-transition: all .5s ease;
	-ms-transition: all .5s ease;
	cursor:pointer
	}
#popups .close a{
	display:block;
	background:#fff;
	width:31px;
	height:31px;
	border-radius:50%;
	line-height:31px;
	margin:0 auto !important
	}
#popups .close a i{
	text-align:center; 
    color:#2a2a2a;
	opacity:.7
	}	
#popups .close:hover a i{
	
    color:#000;
	opacity:1;
	transition: all .5s ease;
	-moz-transition: all .5s ease;
	-webkit-transition: all .5s ease;
	-o-transition: all .5s ease;
	-ms-transition: all .5s ease;
	
	}
#popups .close:hover{
	background:#4c4c4c;
	transform:rotate(360deg);
	-webkit-transform:rotate(360deg);
	-moz-transform:rotate(360deg);
	-o-transform:rotate(360deg);
	-webkit-transform:rotate(360deg);
	}	
	
#shirt_viewport{
	text-align:center;
	position:fixed;
	height:100%;
    right:167px;
	top:57px
	}	

#right_panel{
	float:right;
	width:190px;
	padding:0 30px 0 0;
	margin-right:50px;
	position:relative;
	display: table;
	height: 100%;
	right: 0;
	z-index: 9;
	}

.rightPanelInner{
	display: table-cell;
	vertical-align: middle;
	position: relative;
}


/*6.8.16*/

#viewport .steps_bar .steps_scroll li p{
	margin:-10px 0 0 0;
	line-height:15px
	}


#viewport .steps_bar .steps_scroll{
	width:100%
	}


#popups .popup{
	height:530px
	}
	
#popups .popup .popup_inner{
	background:#fff;
	}
#popups .popup .popup_inner .header{
	background:#2a2a2a;
	height:60px;
	position: relative;
	}
#popups .popup .popup_inner .header h4{
	color:#fff;
	text-align:center;
	line-height:60px;
	font-size:25px;
	word-spacing: 0 !important;
	}

#popups .popup .popup_inner .popup_body{
	padding:10px;
	position:relative;
	height:462px;
	overflow:hidden;
	width:auto;
	}

#viewport .stepsbar_wrapper{
	
	position:relative;
	z-index:99999;
	}
#popups .popup .popup_inner .popup_body ul li{
	cursor:pointer;
	padding:10px;
	border-bottom:1px dotted #ccc;
	transition: all .5s ease;
	-moz-transition: all .5s ease;
	-webkit-transition: all .5s ease;
	-o-transition: all .5s ease;
	-ms-transition: all .5s ease;
	}	

#popups .popup .popup_inner .popup_body ul li> div
{
	display:inline-block;
	vertical-align:middle;
	width:35%;
	}
#popups .popup .popup_inner .popup_body ul li> div:nth-of-type(2){
	width:56%
	}

#popups .popup .popup_inner .popup_body ul li h5{
	font-size:18px;
	transition: all .5s ease;
	-moz-transition: all .5s ease;
	-webkit-transition: all .5s ease;
	-o-transition: all .5s ease;
	-ms-transition: all .5s ease;
	word-spacing: 0 !important;
	}
#popups .popup .popup_inner .popup_body ul li .code{
